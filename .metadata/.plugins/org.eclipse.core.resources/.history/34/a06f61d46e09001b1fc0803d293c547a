package ex_08_10;
// 1-	Informar todos os números de 1000 a 1999 que quando divididos por 11 obtemos resto = 5. (FOR)


public class ex1 {
	public static void main (String args[]) {
		int i, mil = 1000;
		for(i = 0; i <= 1999; i++) {
			if(mil % 11 == 5) {
				System.out.printf("\n %D"i);
			}
		}
	}
	
	
}
